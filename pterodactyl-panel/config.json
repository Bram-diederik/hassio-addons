{
  "name": "pterodactyl Panel Gameserver",
  "version": "1.0.0",
  "slug": "pterodactyl_panel",
  "description": "OpenSource Gameserver",
  "url": "https://github.com/FaserF/hassio-addons/tree/master/pterodactyl",
  "arch": [
    "aarch64",
    "amd64"
  ],
  "startup": "application",
  "boot": "auto",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
  "map": [
    "share:rw",
    "media:rw",
    "ssl"
  ],
  "ingress": true,
  "init": false,
  "panel_icon": "mdi:controller",
  "options": {
    "config_file": null,
    "password": null,
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "config_file": "str",
    "password": "password",
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
    "reset_database": "bool?"
  },
  "ports": {
    "80/tcp": 80,
    "443": 443
  },
  "ports_description": {
    "80/tcp": "Web interface http",
    "443/tcp": "SSL Web interface"
  }
}